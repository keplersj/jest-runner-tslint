{
  "name": "jest-runner-tslint",
  "version": "0.0.0-development",
  "description": "TSLint runner for Jest",
  "main": "dist/index.js",
  "repository": "https://github.com/keplersj/jest-runner-tslint",
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "pretest": "tsc",
    "test": "jest -c jest-tslint.config.js",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "prerelease": "tsc",
    "release": "semantic-release"
  },
  "lint-staged": {
    "*": [
      "jest --bail --findRelatedTests"
    ]
  },
  "release": {
    "verifyConditions": "@semantic-release/github"
  },
  "jest-runner-tslint": {
    "cliOptions": {
      "fix": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/node": "^12.12.7",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "semantic-release": "^15.13.30",
    "ts-jest": "^24.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "cosmiconfig": "^6.0.0",
    "create-jest-runner": "^0.5.0",
    "tslint": "^5.8.0"
  }
}
