{
  "name": "jest-runner-tslint",
  "version": "0.0.0-development",
  "description": "TSLint runner for Jest",
  "main": "dist/index.js",
  "repository": "https://github.com/keplersj/jest-runner-tslint",
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "private": false,
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "pretest": "tsc",
    "test": "jest -c jest-tslint.config.js",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "prerelease": "tsc",
    "release": "semantic-release"
  },
  "lint-staged": {
    "*": ["jest --bail --findRelatedTests"]
  },
  "release": {
    "verifyConditions": "@semantic-release/github"
  },
  "devDependencies": {
    "@commitlint/cli": "5.2.5",
    "@commitlint/config-conventional": "5.2.3",
    "@types/node": "8.5.2",
    "husky": "0.14.3",
    "jest": "22.0.3",
    "lint-staged": "6.0.0",
    "prettier": "1.9.2",
    "semantic-release": "11.0.2",
    "ts-jest": "21.2.4",
    "tslint-config-prettier": "1.6.0",
    "tslint-plugin-prettier": "1.3.0",
    "typescript": "2.6.2"
  },
  "dependencies": {
    "create-jest-runner": "^0.1.1",
    "tslint": "^5.8.0"
  }
}
